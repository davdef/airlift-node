node_name = "test-node"

[producers.test_source]
type = "file"
enabled = true
path = "background.wav"
loop_audio = true
channels = 2
sample_rate = 48000

[processors.simple_pass]
type = "passthrough"
enabled = true

[consumers.test_recorder]
type = "file"
enabled = true
path = "test_simple.wav"

[flows.test_flow]
enabled = true
inputs = ["test_source"]
processors = ["simple_pass"]
outputs = ["test_recorder"]
